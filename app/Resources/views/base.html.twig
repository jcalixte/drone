<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Drone{% endblock %}</title>
        <link href="{{ asset('bootstrap/css/bootstrap.min.css') }}" media="screen, projection" rel="stylesheet" type="text/css" />
        <link href="{{ asset('stylesheets/screen.css') }}" media="screen, projection" rel="stylesheet" type="text/css" />
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {% block javascripts_before %}{% endblock %}
    </head>
    {% block map_load %}<body>{% endblock %}
        <div id="content">
            {% block body %}{% endblock %}
        </div>
        {% block javascripts %}
            <script src="{{ asset('javascript/jquery.min.js') }}" type="text/javascript" charset="utf-8"></script>
        {% endblock %}
    </body>
</html>
